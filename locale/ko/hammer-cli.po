msgstr "매개변수 '%<param>s': %<message>s"

#: ../lib/hammer_cli/clamp.rb:10
msgid "%<env>s: %<message>s"
msgstr "%<env>s: %<message>s"

#: ../lib/hammer_cli/clamp.rb:11
msgid "Unrecognised option '%<switch>s'"
msgstr "인식할 수 없는 옵션 '%<switch>s'"

